<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Setting_F"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:layout_weight="1"
            android:background="@color/white"
            app:contentInsetLeft="0dp"
            app:contentInsetStart="0dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
               >

                <!--<ImageView
                    android:id="@+id/Goback"
                    android:layout_width="@dimen/_9sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:src="@drawable/ic_new_back"
                    android:tint="@color/black" />-->
                <RelativeLayout
                    android:id="@+id/rel_back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/_15sdp">

                    <ImageView
                        android:id="@+id/Goback"
                        android:layout_width="@dimen/_20sdp"
                        android:layout_height="@dimen/_25sdp"
                        android:layout_centerInParent="true"
                        android:padding="@dimen/_5sdp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/ic_new_back"
                        android:tint="@color/black" />
                </RelativeLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_centerVertical="true"
                    android:fontFamily="@font/pop_medium"
                    android:text="Privacy and Settings"
                    android:textColor="@color/black"
                    android:textSize="@dimen/title" />


            </RelativeLayout>

        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/toolbar"
            android:layout_margin="10dp"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/request_verification_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                   
                    android:fontFamily="@font/pop_regular"
                    android:padding="15dp"
                    android:text="Request Verification"
                    android:textColor="@color/black"
                    android:textSize="15dp" />

                <ImageView
                    android:layout_width="@dimen/_13sdp"
                    android:layout_height="@dimen/_13sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_alignParentRight="true"
                    android:rotation="180"
                    android:layout_centerInParent="true"
                    android:src="@drawable/ic_new_back"
                    android:tint="@color/black">

                </ImageView>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/push_notification_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                   
                    android:fontFamily="@font/pop_regular"
                    android:padding="15dp"
                    android:text="Push Notification"
                    android:textColor="@color/black"
                    android:textSize="15dp" />

                <ImageView
                    android:layout_width="@dimen/_13sdp"
                    android:layout_height="@dimen/_13sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_alignParentRight="true"
                    android:rotation="180"
                    android:layout_centerInParent="true"
                    android:src="@drawable/ic_new_back"
                    android:tint="@color/black"></ImageView>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/privacy_setting_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                   
                    android:fontFamily="@font/pop_regular"
                    android:padding="15dp"
                    android:text="Privacy Setting"

                    android:textColor="@color/black"
                    android:textSize="15dp" />

                <ImageView
                    android:layout_width="@dimen/_13sdp"
                    android:layout_height="@dimen/_13sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_alignParentRight="true"
                    android:rotation="180"
                    android:layout_centerInParent="true"
                    android:src="@drawable/ic_new_back"
                    android:tint="@color/black"></ImageView>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/privacy_policy_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                   
                    android:fontFamily="@font/pop_regular"
                    android:padding="15dp"
                    android:text="Privacy Policy"

                    android:textColor="@color/black"
                    android:textSize="15dp" />

                <ImageView
                    android:layout_width="@dimen/_13sdp"
                    android:layout_height="@dimen/_13sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_alignParentRight="true"
                    android:rotation="180"
                    android:layout_centerInParent="true"
                    android:src="@drawable/ic_new_back"
                    android:tint="@color/black"></ImageView>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/terms_condition_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                   
                    android:fontFamily="@font/pop_regular"
                    android:padding="15dp"
                    android:text="Terms &amp; Conditions"
                    android:textColor="@color/black"
                    android:textSize="15dp" />

                <ImageView
                    android:layout_width="@dimen/_13sdp"
                    android:layout_height="@dimen/_13sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_alignParentRight="true"
                    android:rotation="180"
                    android:layout_centerInParent="true"
                    android:src="@drawable/ic_new_back"
                    android:tint="@color/black"></ImageView>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/change_password_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                   
                    android:fontFamily="@font/pop_regular"
                    android:padding="15dp"
                    android:text="Change Password"

                    android:textColor="@color/black"
                    android:textSize="15dp" />

                <ImageView
                    android:layout_width="@dimen/_13sdp"
                    android:layout_height="@dimen/_13sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_alignParentRight="true"
                    android:rotation="180"
                    android:layout_centerInParent="true"
                    android:src="@drawable/ic_new_back"
                    android:tint="@color/black"></ImageView>
            </RelativeLayout>

        </LinearLayout>

    </RelativeLayout>

</FrameLayout>
